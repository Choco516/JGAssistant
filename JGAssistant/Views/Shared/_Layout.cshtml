    <script src="~/Content/js/bootstrap/bootstrap-button.js" type="text/javascript"></script>
    <script src="~/Content/js/bootstrap/bootstrap-collapse.js" type="text/javascript"></script>
    <script src="~/Content/js/bootstrap/bootstrap-carousel.js" type="text/javascript"></script>
    <script src="~/Content/js/bootstrap/bootstrap-typeahead.js" type="text/javascript"></script>
    <script src="~/Content/js/bootstrap/bootstrap-affix.js" type="text/javascript"></script>
    <script src="~/Content/js/bootstrap/bootstrap-datepicker.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery/jquery-tablesorter.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery/jquery-chosen.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery/virtual-tour.js" type="text/javascript"></script>

    <script src="~/Content/js/jquery/jquery-1.8.2.min.js" type="text/javascript"></script>
    <script src="~/Content/js/bootstrap/bootstrap-transition.js" type="text/javascript"></script>
    <link href="~/Content/assets/css/customize-template.css" type="text/css" media="screen, projection" rel="stylesheet" />
    <link href="~/Content/assets/font-awesome-4.3.0/css/font-awesome.min.css" type="text/css" media="screen, projection" rel="stylesheet" />
    <style>
        #body-content {
            padding-top: 40px;
        }
    </style>

    @Styles.Render("~/Content/bootstrapcss")
    @Styles.Render("~/Content/assets/css/rhinoslider-1.05css")
    @Styles.Render("~/Content/menucss")
    @Styles.Render("~/Content/js/bootstrap/bootstrap-dropdown.js")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/js/bootstrap")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrapjs")
