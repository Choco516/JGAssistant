                <a href="@Url.Action("Index", "Home")" class="brand "><i class="fa fa-users"> </i> JGAssistant</a>
                <div id="app-nav-top-bar" class="nav-collapse">
                    <ul class="nav">
                        <li class="dropdown flip-li">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa  fa-anchor "> </i>&nbsp; Solicitudes de embarques
                                <b class="caret hidden-phone"></b>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a href="#">Activas</a>
                                </li>
                                <li>
                                    <a href="#">Procesadas</a>
                                </li>
                            </ul>
