@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <body class="body">
        <div class="navbar navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container">
                    <button class="btn btn-navbar" data-toggle="collapse" data-target="#app-nav-top-bar">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="dashboard.html" class="brand"><i class="icon-user">&nbsp;JarvisGeek</i></a>
                    
                </div>
            </div>
        </div>
        <div id="body-container">
                    <div id="body-content">
                        
                        
            <div class='container'>
                <div class="signin-row row">
                    <div class="span4"></div>
                    <div class="span8">
                        <div class="well well-small well-shadow">
                            <legend class="lead" style="text-align:center;">Bienvenido a JGAssistant</legend>
                            <p style="text-align:left;">Por favor digite sus credenciales</p>
                        </div>
                    </div>
                    <div class="span8"></div>
                </div>
                
                <div class="signin-row row">
                    <div class="span4"></div>
                    <div class="span8">
                        <div class="container-signin">
                            <legend>Credenciales</legend>
                            <form action='dashboard.html' method='POST' id='loginForm' class='form-signin' autocomplete='off'>
                                <div class="form-inner">
                                    <div class="input-prepend">
                                        <span class="add-on" rel="tooltip" title="Username or E-Mail Address" data-placement="top"><i class="fa fa-user "></i></span>
                                        <input type='text' class='span4' id='username'/>
                                    </div>
                                    <div class="input-prepend">
                                        <span class="add-on"><i class="icon-key"></i></span>
                                        <input type='password' class='span4' id='password'/>
                                    </div>
                                    <label class="checkbox" for='remember_me'>Recuerdame
                                        <input type='checkbox' id='remember_me'/>
                                    </label>
                                </div>
                                <footer class="signin-actions">
                                    <input class="btn btn-primary " type='submit' id="submit" value='Ingresar' />
                                </footer>
                            </form>
                        </div>
                    </div>
                    <div class="span3"></div>
                </div>

                
            <!--<div class="span4">

                </div>-->
            </div>
    

            </div>
        </div>

        <footer class="application-footer">
            <div class="container">
                <p>Información Legal Del Sitio</p>
                <div class="disclaimer">
                    <p>© JarvisGeek S.A. Todos los derechos reservados. 2015</p>
                    <div id="app-nav-top-bar" class="nav-collapse">
                        <ul class="menu-footer">
                            <li>
                                <a href="#">Términos de uso</a>
                            </li>
                            <li>
                                <a href="#">Política de privacidad y cookies</a>
                            </li>
                            <li>
                                <a href="#">Términos de pago</a>
                            </li>
                            <li>
                                <a href="#">Acerca de este sitio</a>
                            </li>
                        </ul>
                    </div>
                    
                </div>
            </div>
        </footer>
        <script type="text/javascript">
            $(function(){
                document.forms['loginForm'].elements['j_username'].focus();
                $('body').addClass('pattern pattern-sandstone');
                $("[rel=tooltip]").tooltip();
            });
        </script>
        <script src="~/Content/js/bootstrap/bootstrap-alert.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-modal.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-dropdown.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-scrollspy.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-tab.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-tooltip.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-popover.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-button.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-collapse.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-carousel.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-typeahead.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-affix.js" type="text/javascript"></script>
        <script src="~/Content/js/bootstrap/bootstrap-datepicker.js" type="text/javascript"></script>
        <script src="~/Content/js/jquery/jquery-tablesorter.js" type="text/javascript"></script>
        <script src="~/Content/js/jquery/jquery-chosen.js" type="text/javascript"></script>
        <script src="~/Content/js/jquery/virtual-tour.js" type="text/javascript"></script>
        

	</body>
</html>