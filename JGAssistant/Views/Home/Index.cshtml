@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    #body-content {
        padding-top: 40px;
    }
</style>




<div id="body-container">
    <div id="body-content">
        <section class="nav nav-page">
            <div class="container">
                <div class="row">
                    <div class="span7">
                        <header class="page-header">
                            <h3>
                                Mi asistente
                            </h3>
                        </header>
                    </div>
                </div>
            </div>
        </section>

        <section class="page container">
            <div class="row">
                <div class="span16">
                    <div class="well well-small well-shadow well-anchor-Dashboard ">
                        <legend class="lead">Busquedas y notificaciones</legend>
                        <div class="row">
                            <div class="span5">
                                <div class="box">
                                    <div class="box-header">
                                        <i class="fa fa-search fa-2x"></i>
                                        <h5>Busqueda </h5>
                                    </div>
                                    <div class="box-content">
                                        <form class="form-inline">
                                            <p>Ingrese la palabra clave</p>
                                            <div class="input-prepend">
                                                <span class="add-on"><i class="icon-search"></i></span>
                                                <input class="span4" type="text" placeholder="Número de contenedor, BL, embarque" required>
                                            </div>
                                            <br><br>
                                            <div class="box-footer">
                                                <button type="submit" class="btn btn-primary pull-right">
                                                    <i class="icon-search"></i>
                                                    Buscar
                                                </button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <div class="span5">
                                <div class="box">
                                    <div class="box-header">
                                        <i class="fa fa-clock-o fa-2x"></i>
                                        <h5>S.E. Recientes </h5>
                                    </div>
                                    <div class="blockoff-right">
                                        <ul id="person-list" class="nav nav-list">
                                            <li class="nav-header" style="font-size:14px;">Naviera / Destino / Embarque</li>

                                            <li>
                                                <a href="#">
                                                    <i class="icon-chevron-right pull-right"></i>
                                                    King Ocean / Miami / PH 1860
                                                </a>
                                            </li>

                                            <li>
                                                <a href="#">
                                                    <i class="icon-chevron-right pull-right"></i>
                                                    Crowley / Miami / PH 1859
                                                </a>
                                            </li>

                                            <li>
                                                <a href="#">
                                                    <i class="icon-chevron-right pull-right"></i>
                                                    Isabella / FreePort / PH 1858
                                                </a>
                                            </li>

                                            <li>
                                                <a href="#">
                                                    <i class="icon-chevron-right pull-right"></i>
                                                    Dole / Wilmington / PH 1857
                                                </a>
                                            </li>

                                            <li>
                                                <a href="#">
                                                    <i class="icon-chevron-right pull-right"></i>
                                                    Seaboard / Philadelphia / PH 1856
                                                </a>
                                            </li>

                                            <li>
                                                <a href="#">
                                                    <i class="icon-chevron-right pull-right"></i>
                                                    King Ocean / Miami / PH 1865
                                                </a>
                                            </li>
                                            <li class="active">
                                                <a id="view-all" href="#">
                                                    <i class="icon-chevron-right pull-right"></i>
                                                    <b>Ver todas las solicitudes</b>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="span5">
                                <div class="box">
                                    <div class="box-header">
                                        <i class="fa fa-exclamation-triangle fa-2x"></i>
                                        <h5>Tareas pendientes</h5>
                                    </div>
                                    <div class="box-content">
                                        <div class="btn-group-box">
                                            <button class="btn quick-btn">
                                                <i class="fa fa-ship fa-2x"></i>
                                                <span>Instucción</span>
                                                <span class="etiqueta etiqueta-danger">3</span>
                                            </button>
                                            <button class="btn quick-btn">
                                                <i class="fa fa-certificate fa-2x"></i>
                                                <span>ISF</span>
                                                <span class="etiqueta etiqueta-danger">5</span>
                                            </button>
                                            <button class="btn quick-btn">
                                                <i class="fa fa-flag-checkered fa-2x"></i>
                                                <span>Notificar</span>
                                                <span class="etiqueta etiqueta-danger">7</span>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>

